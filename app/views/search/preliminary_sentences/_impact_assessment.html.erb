<div class="content-prelim-box">
  <div class="content-prelim-inner">
    <div class="content-prelim-text" id="prelim-text">
      An <%= object_display_name_link(object.object_name) %>
      <% unless object.department.blank? %>
        published by the <%= search_link(object.department) %>
      <% end %>
      was received by Parliament on
      <strong><%= format_date(object.date) %></strong>.
    </div>
  </div>
</div>

