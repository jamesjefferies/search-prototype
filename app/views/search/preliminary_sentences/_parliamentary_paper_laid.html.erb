<div class="content-prelim-box">
  <div class="content-prelim-inner">
    <div class="content-prelim-text" id="prelim-text">
      A <%= link_to(object.object_name, search_path(filter: object.type)) %> laid in the
      <%= ses_object_link(object.legislature) %> on
      <strong><%= object.date&.strftime(ApplicationHelper::DATE_DISPLAY_FORMAT) %></strong>
      by <%= ses_object_link(object.lead_member) %> on behalf of the
      <%= ses_object_link(object.department) %>.
    </div>
  </div>
</div>

