<div class="content-prelim-box">
  <div class="content-prelim-inner">
    <div class="content-prelim-text" id="prelim-text">
      A <%= link_to(object.object_name, search_path(filter: object.type)) %> made by <%= ses_object_link(object.correcting_member) %>
      (<%= ses_object_link(object.correcting_member_party) %>) on
      <strong><%= object.correction_date&.strftime(ApplicationHelper::DATE_DISPLAY_FORMAT) %></strong>,
      in the <%= ses_object_link(object.legislature) %> on behalf of the
      <%= ses_object_link(object.department) %>.
    </div>
  </div>
</div>

