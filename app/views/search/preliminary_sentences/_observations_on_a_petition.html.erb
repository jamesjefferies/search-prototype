<div class="content-prelim-box">
  <div class="content-prelim-inner">
    <div class="content-prelim-text" id="prelim-text">
      <%= object_display_name_link(object.object_name) %> in the <%= search_link(object.legislature) %> on
      <strong><%= format_date(object.date) %></strong>
      <% unless object.members.blank? %>
        by <%= render 'search/fragments/list', items: object.members, terminator: ',', singular: false %>
      <% end %>
      on behalf of the <%= search_link(object.department) %>.
    </div>
  </div>
</div>

