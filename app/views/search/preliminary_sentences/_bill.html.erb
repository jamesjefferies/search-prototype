<div class="content-prelim-box">
  <div class="content-prelim-inner">
    <div class="content-prelim-text" id="prelim-text">
      <%= object_display_name_link(object.object_name) %> on
      <strong><%= format_date(object.date_of_order) %></strong>,
      in the <%= render 'search/fragments/list', items: object.legislature, terminator: '', singular: false %>
      <% unless object.member.blank? %>
        sponsored by <%= search_link(object.member) %> (<%= search_link(object.member_party) %>)
        on behalf of the <%= search_link(object.department) %>
      <% end %>
      .
    </div>
  </div>
</div>

